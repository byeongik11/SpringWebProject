.feature-grid .feature:nth-child(odd) .content, .feature-grid .feature:nth-child(odd) .image {
    float: right;
    margin-right: 5%;
    text-align: right;
}
.feature-grid .feature .image {
    width: 30%;
}
.image.rounded {
    border-radius: 100%;
}
.image.rounded img {
    border: solid 0.5em rgba(144, 144, 144, 0.25);
    border-radius: 100%;
}

.image {
    border-radius: 4px;
    border: 0;
    display: inline-block;
    position: relative;
}
.image img {
    display: block;
    width: 100%;
    height: 100%;
}

